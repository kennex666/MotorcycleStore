package bus;

import dao.Kho_DAO;

public class Kho_BUS {
	private Kho_DAO kho_DAO;

	public Kho_BUS() {
		super();
		kho_DAO = new Kho_DAO();
	}
	
	public int getSoLuongById(String maSP) {
		return kho_DAO.getSoLuongById(maSP);
	}
	public void updateQuantity(String maSP, int newSoLuong) {
		kho_DAO.updateQuantity(maSP, newSoLuong);
	}
}
